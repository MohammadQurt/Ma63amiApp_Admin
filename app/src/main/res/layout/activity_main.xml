<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".Main"
    android:background="#ffffff">

    <TextView
        android:text="Ma63amiApp Admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:textSize="20sp"
        android:textColor="#000000"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/menu"
        android:src="@drawable/menu"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/bill"
        android:src="@drawable/bill"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_alignTop="@+id/menu"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/table"
        android:src="@drawable/table"
        android:layout_alignTop="@+id/menu"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="30dp"
        android:layout_marginEnd="30dp" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Bill UI"
        android:id="@+id/textView3"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignLeft="@+id/bill"
        android:layout_alignStart="@+id/bill" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Menu"
        android:id="@+id/textView4"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_below="@+id/table"
        android:layout_alignLeft="@+id/menu"
        android:layout_alignStart="@+id/menu" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Tables"
        android:id="@+id/textView5"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignRight="@+id/table"
        android:layout_alignEnd="@+id/table" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/offer"
        android:layout_marginTop="24dp"
        android:src="@drawable/offer"
        android:layout_below="@+id/textView4"
        android:layout_alignRight="@+id/restInfo"
        android:layout_alignEnd="@+id/restInfo" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/order"
        android:src="@drawable/order"
        android:layout_alignTop="@+id/offer"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/gallary"
        android:src="@drawable/gallery"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignTop="@+id/offer" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Orders"
        android:id="@+id/textView6"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_below="@+id/gallary"
        android:layout_alignLeft="@+id/order"
        android:layout_alignStart="@+id/order" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Offers"
        android:id="@+id/textView7"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignLeft="@+id/offer"
        android:layout_alignStart="@+id/offer" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Gallary"
        android:id="@+id/textView8"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignRight="@+id/gallary"
        android:layout_alignEnd="@+id/gallary" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/feedback"
        android:layout_marginTop="23dp"
        android:src="@drawable/feedback2"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/payment"
        android:src="@drawable/payment"
        android:layout_alignTop="@+id/feedback"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/statistics"
        android:src="@drawable/status"
        android:layout_alignTop="@+id/feedback"
        android:layout_alignLeft="@+id/table"
        android:layout_alignStart="@+id/table" />

    <TextView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="Payments"
        android:id="@+id/textView9"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_below="@+id/payment"
        android:layout_alignLeft="@+id/payment"
        android:layout_alignStart="@+id/payment" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Feedback"
        android:id="@+id/textView10"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_below="@+id/statistics"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:text="Statistics "
        android:id="@+id/textView11"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_alignTop="@+id/textView10"
        android:layout_alignLeft="@+id/statistics"
        android:layout_alignStart="@+id/statistics" />

    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/restInfo"
        android:layout_marginTop="23dp"
        android:src="@drawable/info"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Restaurant Information"
        android:id="@+id/textView2"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textIsSelectable="true"
        android:gravity="center"
        android:layout_below="@+id/restInfo"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/exit"
        android:src="@drawable/exit"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
